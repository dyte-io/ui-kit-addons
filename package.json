{
  "name": "@dytesdk/ui-kit-addons",
  "version": "1.1.5",
  "description": "A collection of Dyte UI-kit addons",
  "type": "module",
  "exports": {
    "./custom-controlbar-button": {
      "import": "./dist/custom-controlbar-button.js",
      "require": "./dist/custom-controlbar-button.cjs",
      "default": "./dist/custom-controlbar-button.cjs",
      "types": "./dist/esm/custom-controlbar-button/index.d.ts"
    },
    "./hand-raise": {
      "import": "./dist/hand-raise.js",
      "require": "./dist/hand-raise.cjs",
      "default": "./dist/hand-raise.cjs",
      "types": "./dist/esm/hand-raise/index.d.ts"
    },
    "./video-background": {
      "import": "./dist/esm/video-background.js",
      "require": "./dist/cjs/video-background.cjs",
      "default": "./dist/cjs/video-background.cjs",
      "types": "./dist/esm/video-background/index.d.ts"
    },
    "./participants-tab-action": {
      "import": "./dist/esm/participants-tab-action.js",
      "require": "./dist/cjs/participants-tab-action.cjs",
      "default": "./dist/cjs/participants-tab-action.cjs",
      "types": "./dist/esm/participants-tab-action/index.d.ts"
    },
    "./participants-tab-toggle": {
      "import": "./dist/esm/participants-tab-toggle.js",
      "require": "./dist/cjs/participants-tab-toggle.cjs",
      "default": "./dist/cjs/participants-tab-toggle.cjs",
      "types": "./dist/esm/participants-tab-toggle/index.d.ts"
    },
    "./chat-host-control": {
      "import": "./dist/esm/chat-host-control.js",
      "require": "./dist/cjs/chat-host-control.cjs",
      "default": "./dist/cjs/chat-host-control.cjs",
      "types": "./dist/esm/chat-host-control/index.d.ts"
    },
    "./mic-host-control": {
      "import": "./dist/esm/mic-host-control.js",
      "require": "./dist/cjs/mic-host-control.cjs",
      "default": "./dist/cjs/mic-host-control.cjs",
      "types": "./dist/esm/mic-host-control/index.d.ts"
    },
    "./participant-menu-item": {
      "import": "./dist/esm/participant-menu-item.js",
      "require": "./dist/cjs/participant-menu-item.cjs",
      "default": "./dist/cjs/participant-menu-item.cjs",
      "types": "./dist/esm/participant-menu-item/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "custom-controlbar-button": [
        "./dist/esm/custom-controlbar-button/index.d.ts"
      ],
      "hand-raise": [
        "./dist/esm/hand-raise/index.d.ts"
      ],
      "video-background": [
        "./dist/esm/video-background/index.d.ts"
      ],
      "participants-tab-action": [
        "./dist/esm/participants-tab-action/index.d.ts"
      ],
      "participants-tab-toggle": [
        "./dist/esm/participants-tab-toggle/index.d.ts"
      ],
      "chat-host-control": [
        "./dist/esm/chat-host-control/index.d.ts"
      ],
      "mic-host-control": [
        "./dist/esm/mic-host-control/index.d.ts"
      ],
      "participant-menu-item": [
        "./dist/esm/participant-menu-item/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json",
    "build": "vite build && npm run compile"
  },
  "author": "dyte Inc.",
  "license": "ISC",
  "dependencies": {
    "@dytesdk/video-background-transformer": "1.1.7"
  },
  "main": "index.js",
  "peerDependencies": {
    "@dytesdk/ui-kit": "*"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^4.4.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyte-in/ui-kit-addons.git"
  },
  "bugs": {
    "url": "https://github.com/dyte-in/ui-kit-addons/issues"
  },
  "homepage": "https://github.com/dyte-in/ui-kit-addons#readme"
}
