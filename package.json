{
  "name": "@cloudflare/realtimekit-ui-addons",
  "version": "0.0.2",
  "description": "A collection of Cloudflare RealtimeKit UI addons",
  "type": "module",
  "exports": {
    "./custom-controlbar-button": {
      "import": "./dist/custom-controlbar-button.js",
      "require": "./dist/custom-controlbar-button.cjs",
      "default": "./dist/custom-controlbar-button.cjs",
      "types": "./dist/esm/custom-controlbar-button/index.d.ts"
    },
    "./hand-raise": {
      "import": "./dist/hand-raise.js",
      "require": "./dist/hand-raise.cjs",
      "default": "./dist/hand-raise.cjs",
      "types": "./dist/esm/hand-raise/index.d.ts"
    },
    "./video-background": {
      "import": "./dist/video-background.js",
      "require": "./dist/video-background.cjs",
      "default": "./dist/video-background.cjs",
      "types": "./dist/esm/video-background/index.d.ts"
    },
    "./participants-tab-action": {
      "import": "./dist/participants-tab-action.js",
      "require": "./dist/participants-tab-action.cjs",
      "default": "./dist/participants-tab-action.cjs",
      "types": "./dist/esm/participants-tab-action/index.d.ts"
    },
    "./participants-tab-toggle": {
      "import": "./dist/participants-tab-toggle.js",
      "require": "./dist/participants-tab-toggle.cjs",
      "default": "./dist/participants-tab-toggle.cjs",
      "types": "./dist/esm/participants-tab-toggle/index.d.ts"
    },
    "./participant-tile-menu": {
      "import": "./dist/participant-tile-menu.js",
      "require": "./dist/participant-tile-menu.cjs",
      "default": "./dist/participant-tile-menu.cjs",
      "types": "./dist/esm/participant-tile-menu/index.d.ts"
    },
    "./chat-host-control": {
      "import": "./dist/chat-host-control.js",
      "require": "./dist/chat-host-control.cjs",
      "default": "./dist/chat-host-control.cjs",
      "types": "./dist/esm/chat-host-control/index.d.ts"
    },
    "./mic-host-control": {
      "import": "./dist/mic-host-control.js",
      "require": "./dist/mic-host-control.cjs",
      "default": "./dist/mic-host-control.cjs",
      "types": "./dist/esm/mic-host-control/index.d.ts"
    },
    "./camera-host-control": {
      "import": "./dist/camera-host-control.js",
      "require": "./dist/camera-host-control.cjs",
      "default": "./dist/camera-host-control.cjs",
      "types": "./dist/esm/camera-host-control/index.d.ts"
    },
    "./participant-menu-item": {
      "import": "./dist/participant-menu-item.js",
      "require": "./dist/participant-menu-item.cjs",
      "default": "./dist/participant-menu-item.cjs",
      "types": "./dist/esm/participant-menu-item/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "custom-controlbar-button": [
        "./dist/esm/custom-controlbar-button/index.d.ts"
      ],
      "hand-raise": [
        "./dist/esm/hand-raise/index.d.ts"
      ],
      "video-background": [
        "./dist/esm/video-background/index.d.ts"
      ],
      "participants-tab-action": [
        "./dist/esm/participants-tab-action/index.d.ts"
      ],
      "participants-tab-toggle": [
        "./dist/esm/participants-tab-toggle/index.d.ts"
      ],
      "chat-host-control": [
        "./dist/esm/chat-host-control/index.d.ts"
      ],
      "mic-host-control": [
        "./dist/esm/mic-host-control/index.d.ts"
      ],
      "camera-host-control": [
        "./dist/esm/camera-host-control/index.d.ts"
      ],
      "participant-menu-item": [
        "./dist/esm/participant-menu-item/index.d.ts"
      ],
      "participant-tile-menu": [
        "./dist/esm/participant-tile-menu/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json",
    "build": "vite build && npm run compile",
    "dev": "npm run build && vite"
  },
  "author": "Cloudflare Inc.",
  "license": "UNLICENSED",
  "dependencies": {
    "@cloudflare/realtimekit-virtual-background": "0.0.2-staging.2"
  },
  "main": "index.js",
  "peerDependencies": {
    "@cloudflare/realtimekit-ui": "1.0.0-staging.14",
    "@cloudflare/realtimekit": "0.5.0-staging.112"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^4.4.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtk-io/ui-kit-addons.git"
  },
  "bugs": {
    "url": "https://github.com/rtk-io/ui-kit-addons/issues"
  },
  "homepage": "https://github.com/rtk-io/ui-kit-addons#readme"
}
