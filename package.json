{
  "name": "@dytesdk/ui-kit-addons",
  "version": "1.0.2",
  "description": "A collection of Dyte UI-kit addons",
  "type": "module",
  "exports": {
    "./custom-controlbar-button": {
      "import": "./dist/esm/custom-controlbar-button/index.js",
      "require": "./dist/cjs/custom-controlbar-button/index.js",
      "default": "./dist/cjs/custom-controlbar-button/index.js",
      "types": "./dist/esm/custom-controlbar-button/index.d.ts"
    },
    "./hand-raise": {
      "import": "./dist/esm/hand-raise/index.js",
      "require": "./dist/cjs/hand-raise/index.js",
      "default": "./dist/cjs/hand-raise/index.js",
      "types": "./dist/esm/hand-raise/index.d.ts"
    },
    "./video-background": {
      "import": "./dist/esm/video-background/index.js",
      "require": "./dist/cjs/video-background/index.js",
      "default": "./dist/cjs/video-background/index.js",
      "types": "./dist/esm/video-background/index.d.ts"
    },
    "./participants-tab-action": {
      "import": "./dist/esm/participants-tab-action/index.js",
      "require": "./dist/cjs/participants-tab-action/index.js",
      "default": "./dist/cjs/participants-tab-action/index.js",
      "types": "./dist/esm/participants-tab-action/index.d.ts"
    },
    "./participants-tab-toggle": {
      "import": "./dist/esm/participants-tab-toggle/index.js",
      "require": "./dist/cjs/participants-tab-toggle/index.js",
      "default": "./dist/cjs/participants-tab-toggle/index.js",
      "types": "./dist/esm/participants-tab-toggle/index.d.ts"
    },
    "./chat-host-control": {
      "import": "./dist/esm/chat-host-control/index.js",
      "require": "./dist/cjs/chat-host-control/index.js",
      "default": "./dist/cjs/chat-host-control/index.js",
      "types": "./dist/esm/chat-host-control/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "custom-controlbar-button": [
        "./dist/esm/custom-controlbar-button/index.d.ts"
      ],
      "hand-raise": [
        "./dist/esm/hand-raise/index.d.ts"
      ],
      "video-background": [
        "./dist/esm/video-background/index.d.ts"
      ],
      "participants-tab-action": [
        "./dist/esm/participants-tab-action/index.d.ts"
      ],
      "participants-tab-toggle": [
        "./dist/esm/participants-tab-toggle/index.d.ts"
      ],
      "chat-host-control": [
        "./dist/esm/participants-tab-toggle/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json",
    "build:clean": "rm -rf ./dist",
    "build": "tsc && npm run build:clean && npm run compile"
  },
  "author": "dyte Inc.",
  "license": "ISC",
  "peerDependencies": {
    "@dytesdk/ui-kit": "*"
  },
  "dependencies": {
    "@dytesdk/video-background-transformer": "^1.1.4"
  },
  "main": "index.js",
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyte-in/ui-kit-addons.git"
  },
  "bugs": {
    "url": "https://github.com/dyte-in/ui-kit-addons/issues"
  },
  "homepage": "https://github.com/dyte-in/ui-kit-addons#readme"
}
